			"douban": true,
			"qq": true,
			"qzone": true,
			"weibo": true,
			"all": [
				"douban",
				"facebook",
				"google",
				"hatenaBookmark",
				"instapaper",
				"linkedin",
				"twitter",
				"weibo",
				"messenger",
				"qq",
				"qzone",
				"viber",
				"vk",
				"pocket",
				"stumbleupon",
				"whatsapp"
			]
		}
	}
