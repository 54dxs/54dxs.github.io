{
	"root": "./",
	"title": "54Helper",
	"description": "程序员开发助手-54Helper(Chrome扩展)",
	"author": "深情小建",
	"language": "zh-hans",
	"plugins": [
		"-lunr",
		"-search",
		"search-pro",
		"code",
		"expandable-chapters-small",
		"back-to-top-button",
		"github",
		"emphasize",
		"splitter",
		"-sharing",
		"sharing-plus"
	],
	"pluginsConfig": {
		"code": {
			"copyButtons": true
